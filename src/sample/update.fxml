<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="187.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UpdateController">
   <children>
      <Button fx:id="gobutton" layoutX="38.0" layoutY="126.0" mnemonicParsing="false" onAction="#change" text="Submit" />
      <Button fx:id="exitbutton" layoutX="140.0" layoutY="126.0" mnemonicParsing="false" onAction="#mainmenu" text="Exit to Main Menu" />
      <TextField fx:id="uwu" layoutX="65.0" layoutY="51.0" />
      <Label layoutX="21.0" layoutY="14.0" text="Enter the ID of the Person you wish to change" />
      <RadioButton fx:id="inactiveradio" layoutX="38.0" layoutY="85.0" mnemonicParsing="false" text="Inactive">
         <toggleGroup>
            <ToggleGroup fx:id="group1" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="alumnusradio" layoutX="143.0" layoutY="85.0" mnemonicParsing="false" text="Alumnus" toggleGroup="$group1" />
   </children>
</AnchorPane>
